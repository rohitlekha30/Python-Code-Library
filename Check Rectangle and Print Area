def Area1(len,brd):
    area=1
    area=len*brd
    return area

RectLen=int(input("Enter the length of the Rectangle - "))
RectBrd=int(input("Enter the breadth of the Rectangle - "))

if (RectLen<0 or RectBrd<0):
    if RectLen<0:
        print("Length of a rectangle cannot be negative. Please enter a new value")
    elif RectBrd<0:
        print("Breadth of a Rectanlge cannot be negative. Please enter a new value")
    else:
        pass
else:
    RectArea=Area1(RectLen,RectBrd)
    print(f"Area of the Rectangle is - {RectArea}")
